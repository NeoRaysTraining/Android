/* program to evaluate double expression */

class  Demo
{
	public static void main(String[] args) 
	{
		byte b=1;
		short s=2;
		int i=3;
		int l=4;
		float f=5.0f;
		double d=6.0;
		double D=l*f-i/s*d-b;   //note:if the double value is defined then the resulting output will be double
		System.out.println(D);
	}
}
