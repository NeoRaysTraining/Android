/* program to implement constructor */
class Box
{
	int w,h,d; //instance variables
	Box(int x,int y,int z)
	{
		w=x;  // intializing instance variables
		h=y;
		d=z;
	}
	int volume()
	{
		int l; // bussiness logic
		l=w*h*d;
		System.out.println("value of l is"+l);
		return l;
	}
};

class ConstructorDemo  
{
	public static void main(String[] args) 
	{
		Box b=null;  //declaring and intailzing reference variable
		b= new Box(1,2,3);  // assiging values in constructor while creating object
		System.out.println("value of w in box is"+b.w);
		System.out.println("value of h in box is"+b.h);
		System.out.println("value of d in box is"+b.d);
		b.volume();
	}
}
