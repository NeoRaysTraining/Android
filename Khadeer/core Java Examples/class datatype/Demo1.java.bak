/* program to ipmlement class datatype using hardcoding */

class  Box
{
	int w=5;
	int h=6;
	int d=7;
}
class Demo1
{
	public static void main(String[] args) 
	{
		int z; //z is a variable of type int
		z=5; //intializing the variable with primitive value
		Box b; //declaring a reference variable
		b=new Box(); //crewtaing in instance
		System.out.println("value of w in box b is"+b.w);
		System.out.println("value of h in box b is"+b.h);
		System.out.println("value of d im box b is"+b.d);
		int vol=b.w*b.h*b.d;
		System.out.println("volume of box b is "+vol);
	}
}
