	{
		x=a;
		y=b;
	}
	void fun()
	{
		System.out.println("value of x is "+x);
		System.out.println("value of y is"+y);
	}

};

class Box
{
		int w,h,d;
		Box(int x,int y,int z)
		{
			w=x;
			h=y;
			d=z;
		}
		int volume()
		{
			int l=0;
			l=w*h*d;
			return l;
	}
};

