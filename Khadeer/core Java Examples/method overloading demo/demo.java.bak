class Hello
{
	void test()
	{
		System.out.println("no params");
	}
	void test(int a)
	{
		System.out.println("value of int a is "+a);
	}
	void test(double d)
	{
		System.out.println("value of double d is"+d);
	}
};
class OverloadDemo
{
	public static void main(String[] args) 
	{
		Hello h; //creating reference variable
		h=new Hello(); // creating an object
		h.test();
		h.test(5);
		h.test(9.1);
	}
}
