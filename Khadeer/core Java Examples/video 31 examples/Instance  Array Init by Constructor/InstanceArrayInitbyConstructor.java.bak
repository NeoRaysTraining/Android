/* program to implement an array as instance variable and intialize it by constructor */


class Hello
{
	int i;  //primitive variable
	int x[]; //Array variable
	Hello(int i,int x[])
	{
		this.i=i;
		this.x=x;
	}
}
class  InstanceArrayInitbyConstructor
{
	public static void main(String[] args) 
	{
		int a=5;
		int p[]={100,101,102,103,104};
		Hello h=new Hello(a,p);

		System.out.println(h.i);
		System.out.println(h.x);
		System.out.println(h.x[0]);
		System.out.println(h.x[1]);
		System.out.println(h.x[2]);
		System.out.println(h.x[3]);
		System.out.println(h.x[4]);
	}
}
