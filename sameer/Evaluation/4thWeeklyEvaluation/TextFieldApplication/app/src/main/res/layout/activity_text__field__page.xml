<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent">
<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.sameer.textfieldapplication.Text_Field_Page"
    android:scrollbars="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text Field Test App"
        android:textStyle="bold|italic"
        android:textSize="30sp"
        android:textColor="#003cff"
        android:id="@+id/text"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="text"
        android:textSize="20sp"
        android:id="@+id/edit1"
        android:layout_below="@+id/text"
        android:layout_marginTop="20dp"
        android:inputType="text"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textAutoComplete"
        android:textSize="20sp"
        android:id="@+id/edit2"
        android:layout_below="@+id/edit1"
        android:layout_marginTop="20dp"
        android:inputType="textAutoComplete"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textAutoCorrect"
        android:textSize="20sp"
        android:id="@+id/edit3"
        android:layout_below="@+id/edit2"
        android:layout_marginTop="20dp"
        android:inputType="textAutoCorrect"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textCapCharacters"
        android:textSize="20sp"
        android:id="@+id/edit4"
        android:layout_below="@+id/edit3"
        android:layout_marginTop="20dp"
        android:inputType="textCapCharacters"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textCapSentences"
        android:textSize="20sp"
        android:id="@+id/edit5"
        android:layout_below="@+id/edit4"
        android:layout_marginTop="20dp"
        android:inputType="textCapSentences"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textCapWords"
        android:textSize="20sp"
        android:id="@+id/edit6"
        android:layout_below="@+id/edit5"
        android:layout_marginTop="20dp"
        android:inputType="textCapWords"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textEmailAddress"
        android:textSize="20sp"
        android:id="@+id/edit7"
        android:layout_below="@+id/edit6"
        android:layout_marginTop="20dp"
        android:inputType="textEmailAddress"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textEmailSubject"
        android:textSize="20sp"
        android:id="@+id/edit8"
        android:layout_below="@+id/edit7"
        android:layout_marginTop="20dp"
        android:inputType="textEmailSubject"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textFilter"
        android:textSize="20sp"
        android:id="@+id/edit9"
        android:layout_below="@+id/edit8"
        android:layout_marginTop="20dp"
        android:inputType="textFilter"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textImeMultiLine"
        android:textSize="20sp"
        android:id="@+id/edit10"
        android:layout_below="@+id/edit9"
        android:layout_marginTop="20dp"
        android:inputType="textImeMultiLine"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textLongMessage"
        android:textSize="20sp"
        android:id="@+id/edit11"
        android:layout_below="@+id/edit10"
        android:layout_marginTop="20dp"
        android:inputType="textLongMessage"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textMultiLine"
        android:textSize="20sp"
        android:id="@+id/edit12"
        android:layout_below="@+id/edit11"
        android:layout_marginTop="20dp"
        android:inputType="textMultiLine"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textNoSuggestions"
        android:textSize="20sp"
        android:id="@+id/edit13"
        android:layout_below="@+id/edit12"
        android:layout_marginTop="20dp"
        android:inputType="textNoSuggestions"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textPassword"
        android:textSize="20sp"
        android:id="@+id/edit14"
        android:layout_below="@+id/edit13"
        android:layout_marginTop="20dp"
        android:inputType="textPassword"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textPersonName"
        android:textSize="20sp"
        android:id="@+id/edit15"
        android:layout_below="@+id/edit14"
        android:layout_marginTop="20dp"
        android:inputType="textPersonName"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textPhonetic"
        android:textSize="20sp"
        android:id="@+id/edit16"
        android:layout_below="@+id/edit15"
        android:layout_marginTop="20dp"
        android:inputType="textPhonetic"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textPostalAddress"
        android:textSize="20sp"
        android:id="@+id/edit17"
        android:layout_below="@+id/edit16"
        android:layout_marginTop="20dp"
        android:inputType="textPostalAddress"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textShortMessage"
        android:textSize="20sp"
        android:id="@+id/edit18"
        android:layout_below="@+id/edit17"
        android:layout_marginTop="20dp"
        android:inputType="textShortMessage"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textUri"
        android:textSize="20sp"
        android:id="@+id/edit19"
        android:layout_below="@+id/edit18"
        android:layout_marginTop="20dp"
        android:inputType="textUri"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textVisiblePassword"
        android:textSize="20sp"
        android:id="@+id/edit20"
        android:layout_below="@+id/edit19"
        android:layout_marginTop="20dp"
        android:inputType="textVisiblePassword"
        />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="textWebEditText"
